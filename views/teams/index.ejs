<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>All Teams</title>
  </head>
  <body>
    <nav>
      <a href="/teams">All Teams</a>
      <a href="/teams/new">Create Team</a>
    </nav>
    <h1>5Aside Teams</h1>
    <div class="team-view">
      <% teams.forEach((team) =>{ %>
      <div class="position"><%= team.position %></div>
      <div class="player"><%= team.player %></div>
      <% }); %>
    </div>
  </body>
</html>
